{
    "semi": false,
    "singleQuote": true,
    "printWidth": 150,
    "tabWidth": 2,
    "useTabs": false,
    "proseWrap": "preserve",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "ignore",
    "ignorePath": ".prettierignore",
    "requireConfig": false,
    "trailingComma": "none"
}